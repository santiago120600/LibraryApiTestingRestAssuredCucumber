{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "mvn install",
            "type": "shell",
            "command": "mvn",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "args": [
                "clean",
                "install",
                "-DskipTests"
            ],
            "problemMatcher": []
        },
        {
            "label": "docker compose up build",
            "type": "shell",
            "command": "docker-compose",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "args": [
                "up",
                "--build",
                "-d"
            ],
            "problemMatcher": []
        },
         {
            "label": "docker compose up",
            "type": "shell",
            "command": "docker-compose",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "args": [
                "up",
                "-d"
            ],
            "problemMatcher": []
        },
    ]
}